<UserControl x:Class="Org.Cen.RobotManager.Controls.Serial.SerialControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">
    <Grid>
        <GroupBox Header=" Serial " HorizontalAlignment="Left" VerticalAlignment="Top" Height="90" Width="290">
            <Grid Margin="0,0,-12,0">
                <Label Content="Test Count : " HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="76" Margin="10,8,0,0"/>
                <TextBox x:Name="TestCountTextBox" HorizontalAlignment="Left" Height="23" Margin="91,8,0,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top" Width="65"/>
                <Button x:Name="IntensiveSerialTestButton" Content="Intensive Check" HorizontalAlignment="Left" Margin="7,35,0,0" VerticalAlignment="Top" Width="109" Click="IntensiveSerialTestButton_Click"/>
                <Label x:Name="IntensiveSerialTestResultLabel" Content="-" HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="159" Margin="121,36,0,0"/>
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>

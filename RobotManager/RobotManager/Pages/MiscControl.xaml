<UserControl x:Class="Org.Cen.RobotManager.Pages.MiscControl"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:pages="clr-namespace:Org.Cen.RobotManager.Pages"
      xmlns:clock="clr-namespace:Org.Cen.RobotManager.Controls.Clock"
      xmlns:kinematics="clr-namespace:Org.Cen.RobotManager.Controls.Robot.Kinematics"

      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="750">

    <Grid x:Name="MiscGrid" Background="#FFE5E5E5" Margin="0,0,0,0">
        <clock:ClockControl x:Name="ClockControl"/>
        <kinematics:RobotKinematicsControl Margin="263,10,-113,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
